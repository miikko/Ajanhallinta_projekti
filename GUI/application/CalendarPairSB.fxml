            <Button id="searchButton" layoutX="470.0" layoutY="475.0" mnemonicParsing="false" text="Search">
               <font>
                  <Font name="Arial" size="20.0" />
               </font>
            </Button>
         </children>
      </Pane>
   </center>
</BorderPane>
