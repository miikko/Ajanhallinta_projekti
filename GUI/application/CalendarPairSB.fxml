                  <Font name="Arial" size="20.0" />
               </font>
            </Button>
            <Label layoutX="252.0" layoutY="326.0" text="Select starting date">
               <font>
                  <Font name="Arial" size="20.0" />
               </font>
            </Label>
            <Label layoutX="618.0" layoutY="326.0" text="Select end date">
               <font>
                  <Font name="Arial" size="20.0" />
               </font>
            </Label>
            <MenuButton id="optMenu" layoutX="835.0" layoutY="20.0" mnemonicParsing="false" prefHeight="30.0" prefWidth="150.0" text="Options">
