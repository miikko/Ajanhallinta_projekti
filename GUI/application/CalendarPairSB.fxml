            <Button id="homeButton" layoutX="15.0" layoutY="20.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="125.0" text="Home">
               <font>
                  <Font name="Arial" size="20.0" />
               </font>
            </Button>
            <Button id="historyButton" layoutX="15.0" layoutY="75.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="125.0" text="History">
